{
  "$schema": "https://indiastack.in/dpi/schema/infrastructure.json",
  "meta": {
    "system_id": "urn:in:gov:uidai:aadhaar",
    "version": "2.0",
    "deployment": "production",
    "lifecycle": "active",
    "license": "PROPRIETARY",
    "maintainer": "help@uidai.gov.in"
  },

  "dependencies": {
    "docs": "https://uidai.gov.in/en/ecosystem/authentication-devices-documents.html",
    "closed_components": [
      "idemia-abis",
      "nec-abis",
      "morpho-sdk",
      "l1-biometric-devices",
      "asa-encryption-module"
    ]
  },

  "access": {
    "endpoint": "https://auth.uidai.gov.in/",
